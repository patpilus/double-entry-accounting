{
  "name": "patpilus/double-entry-accouting",
  "description": "Double entry accounting example",
  "license": "MIT",
  "authors": [
    {
      "name": "Patryk PiluÅ›",
      "email": "patpilus@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/Unit"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "clean": "rm -rf build/ vendor/",
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
